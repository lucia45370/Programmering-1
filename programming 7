def prime(nr):
    for i in range(2,nr):
        
        if nr % i == 0:
            return'false'
    
    return 'true'
    
    # TODO Returnera True om nr är ett primtal, annars returnera false
    # Ex: 5 in - True ut
    pass

prime(2)
prime(3)
print(prime(37))
